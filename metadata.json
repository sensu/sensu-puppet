{
  "name": "sensu-sensu",
  "version": "2.62.2",
  "author": "sensu",
  "summary": "A module to install the Sensu monitoring framework",
  "license": "MIT",
  "source": "https://github.com/sensu/sensu-puppet",
  "project_page": "https://github.com/sensu/sensu-puppet",
  "issues_url": "https://github.com/sensu/sensu-puppet/issues",
  "tags": [
    "sensu",
    "monitoring"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Darwin",
      "operatingsystemrelease": [
        "16"
      ]
    },
    {
      "operatingsystem": "Windows",
      "operatingsystemrelease": [
        "Server 2008 R2",
        "Server 2012 R2"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "14.04",
        "16.04",
        "18.04"
      ]
    },
    {
      "operatingsystem": "Amazon",
      "operatingsystemrelease": [
        "2018.03",
        "2"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">=5.0.0 < 7.0.0"
    }
  ],
  "dependencies": [
    {
      "name": "lwf/remote_file",
      "version_requirement": ">= 1.0.0 <2.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">=4.24.0 <6.0.0"
    }
  ]
}
